{
  "name": "jaas-dashboard",
  "version": "0.0.1",
  "description": "A dashboard for JAAS (Juju as a service)",
  "bugs": {
    "url": "https://github.com/canonical-web-and-design/jaas-dashboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canonical-web-and-design/jaas-dashboard.git"
  },
  "license": "LGPL-3.0",
  "author": {
    "name": "Canonical Webteam",
    "email": "webteam@canonical.com"
  },
  "main": "index.js",
  "scripts": {
    "build": "react-scripts build",
    "clean": "./scripts/clean-files",
    "eslint-check": "eslint src/",
    "lint": "yarn run eslint-check && yarn run prettier-check && yarn run stylelint-check",
    "prettier-check": "prettier --check 'src/**/*'",
    "serve": "yarn run start",
    "start": "react-scripts start",
    "stylelint-check": "stylelint 'src/**/*.scss'",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@canonical/juju-react-components)/)"
    ]
  },
  "dependencies": {
    "@canonical/juju-react-components": "^0.5.1",
    "@canonical/jujulib": "1.3.0",
    "@canonical/macaroon-bakery": "0.3.0",
    "async-limiter": "^1.0.1",
    "classnames": "^2.2.6",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "immer": "4.0.2",
    "node-sass": "4.13.0",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "react-test-renderer": "16.11.0",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "2.3.0",
    "reselect": "^4.0.0",
    "vanilla-framework": "2.4.1",
    "xterm": "4.1.0",
    "xterm-addon-fit": "0.2.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "prettier": "1.18.2",
    "redux-devtools": "3.5.0",
    "stylelint": "11.1.1",
    "stylelint-config-standard": "19.0.0",
    "stylelint-order": "3.1.1"
  }
}
